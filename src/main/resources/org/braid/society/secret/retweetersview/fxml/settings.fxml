<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity" minWidth="-Infinity" prefHeight="540.0" prefWidth="960.0"
  xmlns="http://javafx.com/javafx/8"
  fx:controller="org.braid.society.secret.retweetersview.lib.controls.SettingController"
  fx:id="settingAnchorPane">
  <children>
    <Label alignment="CENTER" prefHeight="25.0" prefWidth="960.0" text="RetweetersView"
      fx:id="settingTitle">
      <font>
        <Font size="20.0"/>
      </font>
    </Label>
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="30.0" prefHeight="509.0"
      prefWidth="160.0">
      <children>
        <Button alignment="CENTER" mnemonicParsing="false"
          onAction="#onRevealThoughtsSettingClicked"
          prefHeight="86.0" prefWidth="160.0" text="Reveal Thoughts"
          textAlignment="CENTER" wrapText="true" fx:id="btnRevealThoughtsSetting">
          <font>
            <Font size="16.0"/>
          </font>
        </Button>
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#onYourTimeLineSettingClicked"
          prefHeight="86.0" prefWidth="160.0" text="Your TimeLine"
          textAlignment="CENTER" wrapText="true" fx:id="btnYourTimeLineSetting">
          <font>
            <Font size="16.0"/>
          </font>
        </Button>
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#onYourTweetsSettingClicked"
          prefHeight="86.0" prefWidth="160.0" text="Your Tweets"
          textAlignment="CENTER" wrapText="true" fx:id="btnYourTweetsSetting">
          <font>
            <Font size="16.0"/>
          </font>
        </Button>
        <Button alignment="CENTER" mnemonicParsing="false"
          onAction="#onManageFollowFollowerSettingClicked"
          prefHeight="86.0" prefWidth="160.0" text="Manage Follow/Followers"
          textAlignment="CENTER" wrapText="true" fx:id="btnManageFollowFollowerSetting">
          <font>
            <Font size="16.0"/>
          </font>
        </Button>
        <Button alignment="CENTER" mnemonicParsing="false"
          onAction="#onManageBlocksMutesSettingClicked"
          prefHeight="86.0" prefWidth="160.0" text="Manage Blocks/Mutes"
          textAlignment="CENTER" wrapText="true" fx:id="btnManageBlocksMutesSetting">
          <font>
            <Font size="16.0"/>
          </font>
        </Button>
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#onSettingSettingButtonClicked"
          prefHeight="85.0" prefWidth="160.0" text="Settings"
          textAlignment="CENTER" wrapText="true" fx:id="btnSettingSettings">
          <font>
            <Font size="16.0"/>
          </font>
        </Button>
      </children>
    </VBox>
  </children>

</AnchorPane>
